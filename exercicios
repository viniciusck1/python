#ex1
a = int (input("Digite a: \n"))
b = int (input("Digite b: \n"))
resultado = a + b
print (resultado)

#ex2
a = float(input("Digite a: \n"))
resultado = a * 1000
print (resultado)

#ex3
a = float (input("Digite a distancia (m): "))
b = float (input("Digite o tempo (s): "))
velocidade = ((a / b) * 3.6)
print ( velocidade )
